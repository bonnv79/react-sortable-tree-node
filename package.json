{
  "name": "react-sortable-tree-node",
  "version": "1.2.0",
  "description": "react-sortable-tree-node",
  "author": "bonnv79",
  "license": "MIT",
  "repository": "bonnv79/react-sortable-tree-node",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "start": "start-storybook -p 6006 -s public",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "rimraf dist",
    "deploy": "build-storybook -s public && gh-pages -d storybook-static",
    "build": "npm run clean && cross-env NODE_ENV=production babel src/components --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react-virtualized-tree",
    "virtualized-tree",
    "react-sortable-tree",
    "sortable-tree"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "frontend-collective-react-dnd-scrollzone": "^1.0.2",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.7.2",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-lifecycles-compat": "^3.0.4",
    "react-virtualized": "^9.22.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-dnd": "^7.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/node-logger": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3",
    "react-scripts": "4.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}